<WidthChoicePencil>
    pos_hint: {'top': .7}
    MDBoxLayout:
        orientation: 'vertical'
#        MDLabel:
#            text: 'THICKNESS'
#            halign: 'center'
        MDSlider:
            min: 1
            max: 10
            show_off: False
            value: app.line_width
            on_touch_up: root.change_pencil_width(self.value), root.dismiss()
            on_touch_up: app.line_width = int(self.value)



