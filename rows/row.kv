<CustomButton@Button>
    background_normal: ""

    canvas.after:
        Color:
            rgba: 0, 0, 0, 1
        Line:
            width: 2
            rectangle: self.x, self.y, self.width, self.height


<Row@MDBoxLayout>
    CustomButton:
        id: page_lbl
        font_size: 30
        size_hint_x: .1

    CustomButton:
        id: title_lbl
        multiline: True
        halign: 'center'
        font_size: 25
        size_hint_x: .3
        #background_color: 0.5, 0.5, 0.5, 1


    CustomButton:
        id: total_lbl
        size_hint_x: .1

        #background_color: 0.5, .5, 0.55, 1

    CustomButton:
        id: camera_lbl
        size_hint_x: .1
        #background_color: 1, 0, 0, 1

    ClickableBox:
        id: wig
        md_bg_color: .19, .19, .19, 1
        text: 'tets'
        size_hint_x: None
        width: 400
        on_press: app.focused_row_properties_update(self, root.story_id, root.title, root.backtime)




        canvas.after:


            Color:
                rgba: 0, 0, 0, 1
            Line:
                width: 2
                rectangle: self.x, self.y, self.width, self.height

